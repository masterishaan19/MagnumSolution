<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ishaanGarg">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cellphone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.ishaanGarg.HelpActivity"></activity>
        <activity android:name="com.example.ishaanGarg.Error_Connectivity_Activity" />
        <activity android:name="com.example.ishaanGarg.Chat_Activity" />
        <activity android:name="com.example.ishaanGarg.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>